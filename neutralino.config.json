{
  "applicationId": "com.bastien.devilishdante.colorpicker",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/app/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": false,
    "writeToLogFile": false
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "debug.log",
    "window.*"
  ],
  "globalVariables": {
    "TEST1": "Hello"
  },
  "modes": {
    "window": {
      "title": "color-picker-alone",
      "width": 480,
      "height": 400,
      "minWidth": 480,
      "minHeight": 400,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/app/assets/icons/icon-512.png",
      "enableInspector": false,
      "borderless": true,
      "maximize": false,
      "hidden": false,
      "resizable": false,
      "exitProcessOnClose": true
    },
    "browser": {
      "globalVariables": {
        "TEST": "Test value browser"
      },
      "nativeBlockList": [
        "filesystem.*"
      ]
    },
    "cloud": {
      "url": "/app/#cloud",
      "nativeAllowList": [
        "app.*"
      ]
    },
    "chrome": {
      "width": 800,
      "height": 500,
      "args": "--user-agent=\"Neutralinojs chrome mode\"",
      "nativeBlockList": [
        "filesystem.*",
        "os.*"
      ]
    }
  },
  "cli": {
    "binaryName": "color-picker-alone",
    "resourcesPath": "/app/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/app/assets/js/libs/neutralino.js",
    "binaryVersion": "4.9.0",
    "clientVersion": "3.8.0"
  }
}